<h1> <%=@contact.first_name%> </h1>
<br>
<h1> <%=@contact.last_name%> </h1>
<br>
<h3> <%=@contact.email%> </h3>
<br>
<p> <%=@contact.note%> </p>

<p> ID: <%=@contact.id%> </p>


<form action='/contacts/<%= @contact.id %>' method='post'>
  <input type='hidden' name='_method' value='delete'>
  <input type='submit' value='Delete Contact'>
</form>
